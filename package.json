{
  "name": "apollo-cms",
  "version": "1.11.1",
  "description": "Apollo-client for prisma-cms",
  "main": "lib/App",
  "files": [
    "src",
    "css",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "nwb build-react-component --copy-files",
    "build-app": "react-scripts build",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "clean": "nwb clean-module && nwb clean-demo",
    "start-nwb": "nwb serve-react-demo",
    "start": "PUBLIC_URL=/ react-scripts start --max-old-space-size=2048",
    "types": "tsc --pretty --noEmit",
    "format": "prettier --write .",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "test": "jest --passWithNoTests",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run types"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn format"
    ]
  },
  "dependencies": {
    "@prisma-cms/component": "latest",
    "@prisma-cms/context": "1.3.2",
    "apollo-boost": "^0.1.17",
    "apollo-cache-inmemory": "^1.3.6",
    "apollo-client": "^2.4.3",
    "apollo-link": "^1.1.0",
    "apollo-link-error": "^1.0.5",
    "apollo-link-http": "^1.5.5",
    "apollo-link-http-common": "^0.2.5",
    "apollo-link-ws": "^1.0.5",
    "apollo-utilities": "^1.0.23",
    "babel-runtime": "^6.26.0",
    "extract-files": "^4.0.0",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.7.3",
    "material-ui": "1.0.0-beta.43",
    "material-ui-icons": "^1.0.0-beta.17",
    "prop-types": "^15.6.0",
    "react": "^16.6.0",
    "react-apollo": "^2.0.4",
    "react-dom": "^16.6.0",
    "subscriptions-transport-ws": "^0.9.5"
  },
  "devDependencies": {
    "@prisma-cms/front": "latest",
    "@prisma-cms/front-editor": "latest",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "@types/react-router-dom": "^5.1.6",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "chalk": "^2.4.1",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-deprecate": "^0.7.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "nwb": "0.23.x",
    "prettier": "^2.1.2",
    "react-scripts": "^4.0.0",
    "source-map-explorer": "^2.0.1",
    "ts-jest": "^26.4.4",
    "typescript": "^4.0.5",
    "web-vitals": "^0.2.4"
  },
  "resolutions": {
    "typescript": "^4.0.5",
    "@prisma-cms/context": "1.3.2",
    "graphql": "14.0.2"
  },
  "homepage": "https://github.com/Fi1osof/apollo-cms#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Fi1osof/apollo-cms.git"
  },
  "keywords": [
    "react-component"
  ],
  "directories": {
    "lib": "lib",
    "test": "tests"
  },
  "bugs": {
    "url": "https://github.com/Fi1osof/apollo-cms/issues"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
